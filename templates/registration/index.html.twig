{% extends 'base.html.twig' %}

{% block title %}Inscrivez vous sur LocaJeu
{% endblock %}

{% block body %}
	<div class="container w-25 m-auto mt-5">
		{{ form_start(form) }}
		{{ form_row(form.email) }}
		{{ form_row(form.prenom) }}
		{{ form_row(form.nom) }}
		{{ form_row(form.password) }}
		<button type="submit" class="btn btn-success">S'inscrire</button>
		{{ form_end(form) }}
	</div>

{% endblock %}
